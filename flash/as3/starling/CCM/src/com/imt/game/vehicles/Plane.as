package com.imt.game.vehicles{			import com.imt.assets.Assets;	import com.imt.framework.display.AbstractStarlingDisplay;	import com.imt.framework.display.IDisplay;	import starling.display.Image;	import starling.events.Event;
	
		public class Plane extends AbstractStarlingDisplay implements IDisplay	{				public var groundY:int = 620;		private var clip:Image;//MovieClip;		public var centerX:int = 200;						public function Plane()		{						super();			addEventListener( starling.events.Event.ADDED_TO_STAGE, onEvent);					}						public function initialize(data:Object=null):void		{									if( !initialized )			{								trace( "initialize" );				createArtwork();				initialized = true;							}					}				public function refresh():void		{			//Starling.juggler.add( clip );		}						public function clear():void		{			//starling.core.Starling.juggler.remove( clip );			//Starling.juggler.remove( clip );		}				public function start():void{};		public function intro():void{};		public function outro():void{};		public function cancel():void{};		public function destroy():void{};						private function onEvent(event:Event):void		{									switch( event.type )			{								case Event.ENTER_FRAME:										break;												case starling.events.Event.ADDED_TO_STAGE:					trace(this + " : " + event.type);					removeEventListener( starling.events.Event.ADDED_TO_STAGE, onEvent);					initialize();					break;								default:					trace(this + " : Unhandled event.");								}					}						// Create artwork.		private function createArtwork():void		{						trace(this + " : createArtwork");			clip = new Image( Assets.getTextureAtlas( "assets" ).getTexture( "vehicles/plane/Plane" ) );			//clip = new MovieClip( Graphics.getMemoryPlaneAtlas().getTextures( "plane/Plane_" ), 18 );				//_flame = new MovieClip( Graphics.getMemoryPlaneAtlas().getTextures( "rocket/engine_flame/flame" ), 18 );				clip.x = Math.ceil( -clip.width * 0.5 );			clip.y = Math.ceil( -clip.height * 0.5 );			addChild( clip );			//Starling.juggler.add( clip );			//clip.play();			//starling.core.Starling.juggler.add( hero );			trace(this + " : createArtwork END");					}			}	}