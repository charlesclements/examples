package gui_de{		import flash.text.TextField;	import flash.display.Sprite;				public class SimpleLogger extends Sprite{				public var txt:TextField;				public function SimpleLogger() {			// constructor code			log( "[SimpleLogger] created" ); 											}				public function log(message:*):void		{									var t:String = txt.text = txt.text + message + "\n";			trace( message );											}	}	}